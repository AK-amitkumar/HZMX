<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
        <menuitem id="product_root" name="产品" sequence="1"/>
            <menuitem id="platform_tmpl_menu" parent="product_root" name="平台产品" sequence="1"/>
            <menuitem id="merchant_tmpl_menu" parent="product_root" name="商户产品" sequence="2"/>
            <menuitem id="shop_tmpl_menu" parent="product_root" name="店铺产品" sequence="3"/>

        <menuitem id="ecommerce_menu_root" name="跨境电商" sequence="2"/>
            <menuitem id="account_authorized_menu" parent="ecommerce_menu_root" name="账户授权" sequence="1"/>
            <menuitem id="sale_order_parent_menu" parent="ecommerce_menu_root" name="销售" sequence="3"/>
            <menuitem id="purchase_parent_menu" parent="ecommerce_menu_root" name="采购" sequence="5"/>
            <menuitem id="stock_parent_menu" parent="ecommerce_menu_root" name="仓库" sequence="7"/>
            <menuitem id="log_menu" parent="ecommerce_menu_root" name="日志" sequence="10"/>

        <menuitem id="b2b_acount_root" name="财务" sequence="3"/>
            <menuitem id="account_management_menu" parent="b2b_acount_root" name="财务管理" sequence="1"/>
            <menuitem id="account_clear_menu" parent="b2b_acount_root" name="结算管理" sequence="2"/>
            <menuitem id="account_configuration_menu" parent="b2b_acount_root" name="配置" sequence="3"/>

        <menuitem id="platform_manage_root" name="平台运营" sequence="4"/>
            <menuitem id="acount_manage_parent" name="账号管理" parent="platform_manage_root" sequence="1"/>
            <menuitem id="base_data_menu" name="基础数据" parent="platform_manage_root" sequence="3"/>

        <!--隐藏销售、采购、仓库、会计菜单-->
        <record model="ir.ui.menu" id="sales_team.menu_base_partner">
            <field name="groups_id" eval="[(6,0,[ref('base.group_no_one')])]"/>
        </record>
        <record model="ir.ui.menu" id="purchase.menu_purchase_root">
            <field name="groups_id" eval="[(6,0,[ref('base.group_no_one')])]"/>
        </record>
        <record model="ir.ui.menu" id="stock.menu_stock_root">
            <field name="groups_id" eval="[(6,0,[ref('base.group_no_one')])]"/>
        </record>
        <record model="ir.ui.menu" id="account.menu_finance">
            <field name="groups_id" eval="[(6,0,[ref('base.group_no_one')])]"/>
        </record>
        <!--隐藏设置菜单-->
        <record model="ir.ui.menu" id="base.menu_administration">
            <field name="groups_id" eval="[(6,0,[ref('base.group_no_one')])]"/>
        </record>
        <!--隐藏网站管理菜单-->
        <record model="ir.ui.menu" id="website.menu_website_configuration">
            <field name="groups_id" eval="[(6,0,[ref('base.group_no_one')])]"/>
        </record>
        <!--隐藏网站菜单-->
        <record model="ir.ui.menu" id="website.menu_website">
            <field name="groups_id" eval="[(6, 0, [ref('base.group_no_one')])]"/>
        </record>
	</data>
</odoo>